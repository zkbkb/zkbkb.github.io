{
    "idf.pythonInstallPath": "/Users/zkb/.pyenv/shims/python",
  "triggerTaskOnSave.tasks": {
      "sweetpad: build": [
          "**/*.swift"
      ]
  },
  "triggerTaskOnSave.on": true,
  "triggerTaskOnSave.showNotifications": false,
  "triggerTaskOnSave.restart": true,
  "triggerTaskOnSave.delay": 1000,
  "triggerTaskOnSave.resultIndicatorResetTimeout": 5,
  "workbench.colorCustomizations": {},
  "sweetpad.format.args": [
      "--in-place",
      "--configuration",
      ".vscode/.swift-format",
      "${file}"
  ],
  "[swift]": {
      "editor.defaultFormatter": "sweetpad.sweetpad",
      "editor.formatOnSave": true
  },
  "editor.quickSuggestions": {
    "comments": "off",
    "strings": "off",
    "other": "off"
},
  "swift.path": "/Applications/Xcode.app/Contents/Developer/Toolchains/XcodeDefault.xctoolchain/usr/bin",
  "swift.swiftEnvironmentVariables": {
      "DEVELOPER_DIR": "/Applications/Xcode.app"
  },
  "latex-workshop.latex.tools": [
      {
          "name": "latexmk",
          "command": "/usr/local/texlive/2024basic/bin/universal-darwin/latexmk",
          "args": [
              "-synctex=1",
              "-interaction=nonstopmode",
              "-file-line-error",
              "-pdf",
              "%DOC%"
          ]
      },
      {
          "name": "pdflatex",
          "command": "/usr/local/texlive/2024basic/bin/universal-darwin/pdflatex",
          "args": [
              "-synctex=1",
              "-interaction=nonstopmode",
              "-file-line-error",
              "%DOC%"
          ]
      }
  ],
  "latex-workshop.latex.recipes": [
      {
          "name": "latexmk",
          "tools": [
              "latexmk"
          ]
      },
      {
          "name": "pdflatex",
          "tools": [
              "pdflatex"
          ]
      }
  ],
  "latex-workshop.view.pdf.viewer": "tab",
  "latex-workshop.latex.autoBuild.run": "onSave",
  "latex-workshop.latex.outDir": "%DIR%",
  "idf.pythonInstallPath": "/Users/zkb/.pyenv/shims/python",
  "files.eol": "\n",
  "editor.fontFamily": "\"Maple Mono NF CN\", \"Maple Mono NF\", \"Maple Mono\", \"JetBrains Mono\", \"Fira Code\", \"SF Mono\", Monaco, \"Cascadia Code\", \"Roboto Mono\", Consolas, \"Courier New\", monospace",
  "editor.fontSize": 14,
  "editor.fontWeight": "400",
  "editor.fontLigatures": true,
  "editor.lineHeight": 1.6,
  "editor.smoothScrolling": true,
  "editor.cursorBlinking": "smooth",
  "editor.cursorSmoothCaretAnimation": "on",
  "terminal.integrated.fontFamily": "\"Maple Mono NF CN\", \"Maple Mono NF\", \"Maple Mono\", \"JetBrains Mono\", \"Fira Code\", \"SF Mono\", Monaco, monospace",
  "terminal.integrated.fontSize": 11,
  "terminal.integrated.fontWeight": "400",
  "debug.console.fontFamily": "\"Maple Mono NF CN\", \"Maple Mono NF\", \"Maple Mono\", monospace",
  "debug.console.fontSize": 11,
  "editor.fontVariations": false,
  "editor.renderWhitespace": "selection",
  "editor.guides.bracketPairs": true,
  "editor.bracketPairColorization.enabled": true,
  "problems.showCurrentInStatus": false,
  "files.exclude": {
      "**/.git": true,
      "**/.svn": true,
      "**/.hg": true,
      "**/CVS": true,
      "**/.DS_Store": true,
      "**/Thumbs.db": true,
      "**/__pycache__": true,
      "**/*.pyc": true,
      "**/node_modules": true,
      "**/.venv": true,
      "**/venv": true,
      "**/build": true,
      "**/dist": true
  },
  "search.exclude": {
      "**/node_modules": true,
      "**/bower_components": true,
      "**/*.code-search": true,
      "**/build": true,
      "**/dist": true,
      "**/__pycache__": true,
      "**/.DS_Store": true,
      "**/.git": true,
      "**/.venv": true,
      "**/venv": true
  },
  "editor.rulers": [80, 100, 120],
  "files.associations": {
      "*.ino": "cpp",
      "*.h": "c",
      "*.c": "c",
      "*.cpp": "cpp",
      "Arduino.h": "cpp",
      "math.h": "c",
      "stdint.h": "c",
      "stdbool.h": "c"
  },
  "files.watcherExclude": {
      "**/.git/objects/**": true,
      "**/.git/subtree-cache/**": true,
      "**/node_modules/*/**": true,
      "**/.hg/store/**": true,
      "**/__pycache__/**": true,
      "**/.venv/**": true,
      "**/venv/**": true,
      "**/build/**": true,
      "**/dist/**": true
  }
}
